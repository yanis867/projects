package ehealth_app;

import java.awt.Color;

import javax.swing.ImageIcon;
import javax.swing.JFrame;

public class Main {

	public static void main(String[] args) {
		JFrame frame = new JFrame(); // cr√©er une instance d'un frame (cadre), ceci est invisible: faire \/
		frame.setTitle("cookie's biz");
		frame.setSize(800, 450);
		frame.setResizable(true);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		
		frame.setVisible(true);
		ImageIcon image = new ImageIcon("cookie_cat_yawning.jpg");
		frame.setIconImage(image.getImage());
		frame.getContentPane().setBackground(new Color(212121));
	}

}
